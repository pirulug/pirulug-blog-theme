extends ../layouts/dashboard

append config
  -pageName = 'Subscribers'
  -pageUrl = 'dash_subscribers'

block content
  .card
    .card-header
      h6.card-title.m-0 Subscribers
    .card-body
      table#dt-basic.table.table-border.table-hover.dataTable.no-footer.dtr-inline(aria-describedby='dt-basic_info' style='width: 1185px;')
        thead
          tr
            th.sorting.sorting_asc(tabindex='0' aria-controls='dt-basic' rowspan='1' colspan='1' style='width: 1147px;' aria-sort='ascending' aria-label='E-Mail: activate to sort column descending') E-Mail
        tbody
          tr.odd
            td.dtr-control.sorting_1(tabindex='0') email1@gmail.com


block scripts 
